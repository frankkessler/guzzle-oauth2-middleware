{
    "name": "frankkessler/guzzle-oauth2-middleware",
    "description": "OAuth2 middleware for Guzzle - Built off of Commerceguys/guzzle-oauth2-plugin package",
    "license": "MIT",
    "require": {
        "guzzlehttp/guzzle": "~6.0",
        "lcobucci/jwt": "~3.2"
    },
    "autoload": {
        "psr-4": {
            "Frankkessler\\Guzzle\\Oauth2\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "mockery/mockery": "0.9.*",
        "satooshi/php-coveralls": "1.*"
    },
    "autoload-dev": {
        "psr-4": {
            "Frankkessler\\Guzzle\\Oauth2\\Tests\\": "tests"
        }
    }
}
